<%= form_for @feedback, :url => event_feedbacks_path(@event) do |f| %>

<div class="form-group">
	<%= f.label :name, "What's Your Name?"%>
	<%= f.text_field :name%>
</div>

<div class="form-group">
	<%= f.label :content, "Any Feedback?" %>
	<%= f.text_area :content%>
</div>

<div class="form-group">
	<%= f.submit :class =>"btn btn-primary"%>
</div>

	

<%end%>